<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Colors-->
    <Color x:Key="ColorPrimary1">#20a2cd</Color>
    <Color x:Key="ColorPrimary2">#60c2e3</Color>
    
    <!--Brush-->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{DynamicResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="BrushPrimary2" Color="{DynamicResource ColorPrimary2}"/>
    
</ResourceDictionary>